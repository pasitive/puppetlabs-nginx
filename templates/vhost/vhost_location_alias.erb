  location <%= location %> {
<% if @location_cfg_prepend -%><% location_cfg_prepend.each do |key,value| -%>
    <%= key %> <%= value %>;
<% end -%><% end -%>
<% if @location_alias -%
    alias  <%= location_alias %>;
<% end -%>
<% if @location_cfg_append -%><% location_cfg_append.each do |key,value| -%>
    <%= key %> <%= value %>;
<% end -%><% end -%>
    alias  <%= alias_root %>;
    index <% index_files.each do |i| %> <%= i %> <% end %>; 
  }
